<div class="container-fluid">
    <h1 class="text-center">Holiday Wish List</h1>
    <div class="flexslider custom_slider">
        <ul class="slides" id="fashion_images_container">
            <script id="fashion_images_template" type="x-tmpl-mustache/text">
               <li><img src="{{image_url}}" alt="fashion" /></li>
            </script>
        </ul>
    </div>
</div>

<script>
    $(document).ready(function(e) {
        init(e);
        function renderAll (){
            init_home_hours();
            var fashion = getFashionBySlug('stc-wish-list');
            if (fashion != undefined){
                var fashion_images = fashion.fashion_images
                renderFashionImages("#fashion_images_container", "#fashion_images_template", fashion_images);
            }
            show_content();
            $('.flexslider').flexslider({
                animation: "slide",
            });
        }
        loadMallDataCached(renderAll); 
    })
</script>