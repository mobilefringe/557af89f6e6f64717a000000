<script>
    $(document).ready(function(e) {
        init(e);
        function renderAll (){
            init_home_hours();
            var fashion = getFashionBySlug('stc-wish-list');
            if (fashion != undefined){
                var fashion_images = fashion.fashion_images
                renderFashionImages("#fashion_images_container", "#fashion_images_template", fashion_images);
            }
            show_content();
            $('.flexslider').flexslider({
                animation: "slide",
            });
        }
        loadMallDataCached(renderAll); 
    })
</script>